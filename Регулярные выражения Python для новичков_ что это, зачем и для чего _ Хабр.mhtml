.ActivityIndicator::after { content: ".\a ..\a ..."; display: inline-block;=
 white-space: pre-wrap; }

[dir] .ActivityIndicator::after { transform: translateY(33.33%); }

[dir=3D"ltr"] .ActivityIndicator::after { text-align: left; animation: 1s s=
teps(4) 0s infinite normal none running ActivityIndicatorAnimation; }

[dir=3D"rtl"] .ActivityIndicator::after { text-align: right; animation: 1s =
steps(4) 0s infinite normal none running ActivityIndicatorAnimation; }

@supports ((-webkit-clip-path:inset(0 0)) or (clip-path:inset(0 0))) {
